.container{
  background:#FAFAFA;
  height:1500rpx;
  align-items:unset;
}
.lists{
  width:100%;
  height:100rpx;
}
.list{
  width:100%;
  height:100rpx;
  display:flex;
  flex-direction:row;
  margin:0 auto;
  line-height:100rpx;
  background:#FFFFFF;
  border-bottom:1rpx solid #EEEEEE;
}
.list text{
  color:#666666;
  font-size:30rpx;
  width:80%;
  margin-left:5%;
}
.list image{
